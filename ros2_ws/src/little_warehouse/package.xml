  <url type="repository">https://github.com/larmesto/turtlebot3_coppeliasim</url>
  <url type="bugtracker">https://github.com/larmesto/turtlebot3_coppeliasim/issues</url>
  <author email="leo.armesto@gmail.com">Leopoldo Armesto</author>
  <author email="rinese@upv.edu.es">Ricardo Nu√±ez</author>
  
